#include <stdio.h>

int main() {
    int n;
    
    printf("informe numero: ");
    scanf("%d",&n);
    
    if ((n % 2 == 0 && n % 3 !=0)){
        printf("%d e divisivel por 2 e nao por 3", n);
    } else if ((n % 3 == 0 && n % 2 !=0)){
        printf("%d e divisivel por 3 e nao por 2", n);
    }else {
        printf("%d nao e divisivel por 2 e 3, ou e divisivel por ambos",n);
    }
    return 0;
}
